import javax.swing.JFrame;

public class frame extends JFrame{
Panel panel;
 public frame(){
    panel = new Panel();
    add(panel);
    setSize(500,500);  // tama√±o  (alto,ancho)   
    setDefaultCloseOperation(3);  // terminar el programa al cerrar
    setLocationRelativeTo(null); //centrado en pantalla
 }
}
